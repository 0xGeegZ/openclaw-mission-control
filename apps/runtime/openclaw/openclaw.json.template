{
  "auth": {
    "profiles": {
      "vercel-ai-gateway:default": {
        "provider": "vercel-ai-gateway",
        "mode": "api_key"
      }
    }
  },
  "agents": {
    "defaults": {
      "workspace": "/root/clawd",
      "model": {
        "primary": "vercel-ai-gateway/minimax/minimax-m2.5",
        "fallbacks": [
          "vercel-ai-gateway/anthropic/claude-haiku-4.5",
          "vercel-ai-gateway/moonshotai/kimi-k2.5",
          "vercel-ai-gateway/openai/gpt-5-nano"
        ]
      },
      "models": {
        "vercel-ai-gateway/minimax/minimax-m2.5": { "alias": "Minimax M2.5" },
        "vercel-ai-gateway/anthropic/claude-haiku-4.5": { "alias": "Claude Haiku 4.5" },
        "vercel-ai-gateway/moonshotai/kimi-k2.5": { "alias": "Moonshot Kimi K2.5" },
        "vercel-ai-gateway/openai/gpt-5-nano": { "alias": "GPT-5 Nano" }
      },
      "compaction": { "mode": "safeguard" },
      "contextPruning": { "mode": "cache-ttl", "ttl": "5m" },
      "maxConcurrent": 5,
      "subagents": { "maxConcurrent": 8 }
    }
  },
  "gateway": {
    "port": 18789,
    "mode": "local",
    "http": {
      "endpoints": {
        "responses": { "enabled": true }
      }
    }
  },
  "commands": { "native": "auto", "nativeSkills": "auto" },
  "skills": {
    "install": { "nodeManager": "npm" },
    "entries": {
      "bird": { "enabled": true },
      "clawdhub": { "enabled": true },
      "openai-image-gen": { "enabled": true },
      "openai-whisper-api": { "enabled": true },
      "video-frames": { "enabled": true },
      "cron-gen": { "enabled": true },
      "sql-gen": { "enabled": true },
      "markdown-converter": { "enabled": true },
      "postgres": { "enabled": true },
      "remind-me": { "enabled": true }
    }
  }
}
